<?xml version="1.0" encoding="UTF-8"?>
<form	xmlns="http://www.todoyu.com"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.todoyu.com ../../../../core/config/form.xsd">
	<fieldsets>

		<fieldset name="warning-fieldset-buttons">
			<class>buttons</class>
			<elements>

				<field type="cancelButton" name="cancel">
					<onclick>Todoyu.Popups.closeLast()</onclick>
				</field>

				<field type="saveButton" name="save">
					<onclick>Todoyu.Popups.closeLast(); Todoyu.Ext.calendar.Event.Edit.saveEvent(true); $('event-form').blur()</onclick>
				</field>

			</elements>
		</fieldset>

	</fieldsets>
</form>